{
    "name": "leroy-merlin-br/dployer",
    "description": "A simple script to deploy PHP applications to AWS ElasticBeanstalk",
    "license": "MIT",
    "keywords": [
        "deploy",
        "aws",
        "elasticbeanstalk",
        "ebs"
    ],
    "require": {
        "php": ">=7.0",
        "aws/aws-sdk-php": "~2.8",
        "icecave/isolator": "~2.3",
        "illuminate/container": "~4.2",
        "symfony/console": "~2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~5"
    },
    "autoload": {
        "classmap": [
            "app"
        ]
    },
    "bin": [
        "dployer"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.4.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
